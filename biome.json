{
	"$schema": "https://biomejs.dev/schemas/1.4.1/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"formatter": {
"indentStyle": "tab",
"lineWidth": 80
	},
	"javascript":{
		"formatter": {
			"quoteStyle": "single",
			"trailingComma":"es5"
		}
	},
	
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true
		}
	}
}
